{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "fs:default",
    "fs:allow-exists",
    "fs:allow-read",
    "fs:allow-write",
    "fs:allow-write-file",
    "fs:allow-read-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:scope-document-recursive",
    "shell:default",
    "process:default",
    "sql:allow-load",
    "sql:allow-execute",
    "sql:allow-select",
    "sql:allow-close"
  ]
}